  "scripts": {
    "clean": "rimraf dist",
    "build": "npm run clean && cross-env NODE_ENV=production webpack --config webpack.config.production.js",
    "lint": "eslint --ext .js,.jsx .",
    "deploy": "nnpm run clean & nnpm run build && nsurge -p .",
    "export": "spectacle-renderer --delay 3000",
    "start": "cross-env NODE_ENV=development node server.js",
    "build:spectacle": "cd node_modules/spectacle && npm run clean && npm run build:lib",
    "release": "npm run build:spectacle && npm run build && s3-deploy './dist/**' --bucket assets.eli.wtf/talks/animation-talk-fitc-2018 --cwd './dist/' --profile 'eli.wtf'"
